{
  "name": "lighter-sdk",
  "version": "1.0.0",
  "description": "Comprehensive TypeScript SDK for the Lighter protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lighter/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/lighter/sdk/issues"
  },
  "homepage": "https://github.com/lighter/sdk#readme",
  "keywords": [
    "lighter",
    "zklighter",
    "defi",
    "trading",
    "sdk",
    "typescript",
    "ethereum",
    "zksync",
    "dex",
    "perpetuals"
  ],
  "author": "Lighter Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "ethers": "^6.9.2"
  }
}
